{
	"manifest_version": 3,
	"name": "Chaos Extension",
	"version": "0.1",
	"description": "chaos.",
	"permissions": ["storage"],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"js/effects.js",
				"js/chaos.js"
			],
			"css": ["css/chaos.css"]
		}
	]
}
